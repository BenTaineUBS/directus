{
	"name": "docs",
	"private": true,
	"scripts": {
		"build": "pnpm -r typedocs && vitepress build",
		"dev": "pnpm -r typedocs && vitepress dev",
		"preview": "pnpm -r typedocs && vitepress preview",
		"spellcheck": "spellchecker -p spell indefinite-article repeated-words syntax-mentions syntax-urls frontmatter",
		"update-thumbnail": "thumbsmith deploy .thumbsmith/docs.thumbnail.html || exit 0"
	},
	"dependencies": {
		"node-fetch": "3.3.1"
	},
	"devDependencies": {
		"@thumbsmith/url": "0.1.3",
		"@unhead/vue": "1.1.26",
		"spellchecker-cli": "6.1.1",
		"vitepress": "1.0.0-alpha.75",
		"vue": "3.3.4"
	}
}
