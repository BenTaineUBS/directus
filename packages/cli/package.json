		"app-module-path": "^2.2.0",
		"chalk": "^4.1.0",
		"cli-highlight": "^2.1.11",
		"cli-table3": "^0.6.0",
		"cosmiconfig": "^7.0.0",
		"debug": "^4.3.1",
		"dotenv": "^10.0.0",
		"eventemitter2": "^6.4.4",
		"figlet": "^1.5.0",
		"fs-extra": "^10.0.0",
		"gluegun": "^4.6.1",
		"indent-string": "^5.0.0",
		"jaro-winkler": "^0.2.8",
		"js-yaml": "^4.1.0",
		"json5": "^2.2.0",
		"liquidjs": "^9.25.0",
		"marked": "^2.1.3",
		"marked-terminal": "^4.1.1",
		"merge-options": "^3.0.4",
		"open": "^8.2.0",
		"pretty-ms": "^7.0.1",
